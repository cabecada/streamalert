{
  "id": "prod",
  "region": "us-east-1",
  "modules": {
    "stream_alert": {
      "alert_processor": {
        "timeout": 25,
        "memory": 128,
        "version": 13
      },
      "rule_processor": {
        "timeout": 10,
        "memory": 256,
        "version": 39
      },
      "inputs": {
        "aws-sns:": []
      },
      "outputs": {
        "aws-s3": [],
        "aws-lambda": []
      },
      "vpc_config": {
        "subnet_ids": [],
        "security_group_ids": []
      }
    },
    "cloudwatch_monitoring": {
      "enabled": true
    },
    "kinesis": {
      "streams": {
        "shards": 5,
        "retention": 36
      },
      "firehose": {
        "enabled": true,
        "s3_bucket_suffix": "streamalert.results"
      }
    },
    "kinesis_events": {
      "enabled": true,
    },
    "cloudtrail": {
      "enabled": true,
      "event_pattern": {
        "source": [],
        "detail-type": [],
        "detail": {}
      }
    },
    "flow_logs": {
      "enabled": true,
      "targets": {
        "vpcs": [],
        "subnets": [],
        "enis": []
      }
    }
  },
  "outputs": [
    "kinesis_username",
    "kinesis_access_key",
    "kinesis_secret_key"
  ],
  "__version__": 3
}